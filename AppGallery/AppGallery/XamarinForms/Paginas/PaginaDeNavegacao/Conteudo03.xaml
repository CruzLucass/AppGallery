<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppGallery.XamarinForms.Paginas.PaginaDeNavegacao.Conteudo03"
             NavigationPage.HasBackButton="False"
             Title="Conteudo 03">
    
    <ContentPage.Content>
        <StackLayout>

            <Button
                Text="Inserir pagina na Pilha"
                Clicked="InserirPaginaPilha"
            />

            <Button
                Text="Remover pagina da Pilha"
                Clicked="RemoverPaginaPilha"
            />

            <Label Text="Pagina de Coteudo 03"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" 
            />

            <Button
                Text="Voltar"
                Clicked="VoltarParaPaginaAnterior"
            />

            <Button
                Text="Voltar Para o inicio"
                Clicked="VoltarParaOInicio"
            />
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>