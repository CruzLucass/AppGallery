<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppGallery.XamarinForms.Paginas.PaginaDeNavegacao.Conteudo02"
             Title="Conteudo 02">
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Pagina de Conteudo 02"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" 
            />

            <Button
                Text="PopAsync"
                Clicked="VoltarParaPaginaAnterior"
            />

            <Button
                Text="PushAsync"
                Clicked="AbrirPagina03"
            />

        </StackLayout>
    </ContentPage.Content>
</ContentPage>